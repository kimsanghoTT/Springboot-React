<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kh.mapper.BCUserMapper">
	<insert id="saveUser" parameterType="com.kh.dto.BCUser">
		insert into bcuser (email, password, username) values (#{email}, #{password}, #{username})
	</insert>
</mapper>